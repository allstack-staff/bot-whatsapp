#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/gkigor/Development/asc/bot/bot-compiled/bot-whatsapp/node_modules/pino/node_modules:/home/gkigor/Development/asc/bot/bot-compiled/bot-whatsapp/node_modules:/home/gkigor/Development/asc/bot/bot-compiled/node_modules:/home/gkigor/Development/asc/bot/node_modules:/home/gkigor/Development/asc/node_modules:/home/gkigor/Development/node_modules:/home/gkigor/node_modules:/home/node_modules:/node_modules:/home/gkigor/Development/asc/bot/bot-compiled/bot-whatsapp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/gkigor/Development/asc/bot/bot-compiled/bot-whatsapp/node_modules/pino/node_modules:/home/gkigor/Development/asc/bot/bot-compiled/bot-whatsapp/node_modules:/home/gkigor/Development/asc/bot/bot-compiled/node_modules:/home/gkigor/Development/asc/bot/node_modules:/home/gkigor/Development/asc/node_modules:/home/gkigor/Development/node_modules:/home/gkigor/node_modules:/home/node_modules:/node_modules:/home/gkigor/Development/asc/bot/bot-compiled/bot-whatsapp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pino/bin.js" "$@"
else
  exec node  "$basedir/../pino/bin.js" "$@"
fi
